{
    "border-radius": [
        "webkit"
        ],
    "box-shadow": [
        "webkit"
        ],
    // Not used in most browsers, so add them later
    // border-image-source, border-image-slice, border-image-width, border-image-outset, border-image-repeat
    "border-image": [
        "webkit",
        "moz",
        "o"
        ],

    "box-sizing": [
        "webkit",
        "moz"
        ],
    // TODO: either double them or add some syntax to ease it
    // animation-play-state, animation-name, animation-duration, animation-timing-function, animation-delay, animation-iteration-count, animation-direction
    "animation": [
        "webkit",
        "moz",
        "ms",
        "o"
        ],
    "transform": [
        "webkit",
        "moz",
        "ms",
        "o"
        ],
    "transform-origin": [
        "webkit",
        "moz",
        "ms",
        "o"
        ],
    // TODO: either double them or add some syntax to ease it
    // transition-property, transition-duration, transition-delay, transition-timing-function
    "transition": [
        "webkit",
        "moz",
        "ms",
        "o"
        ],
    "background-clip": [
        "o" // Opera lte 10.1
        ],
    "background-origin": [
        "o" // Opera lte 10.1
        ],
    "background-size": [
        "o" // Opera lte 10.1
        ],
    // TODO: either double them or add some syntax to ease it
    // columns-width, columns-count, column-gap, column-rule, column-rule-width, column-rule-style, column-rule-color, column-span, column-fill
    "columns": [
    	"webkit",
        "moz"
        ],
    "<image>": [
        "webkit",
        "moz",
        "ms",
        "o"
        ],
    "tab-size": [
    	"moz",
        "o"
        ],
    "hyphens": [
    	"webkit",
        "moz"
        ]
}
